<div class="col-md-10" style="background-color:#FFF;">
    <div class="container-fluid">
        <div class="row footer-row1" style="padding-top:5px;">
            <form>
                <div class="col-md-6">
                    <div class="form-group">
                        <label for="inputPassword" class="col-sm-3 control-label padding-txt">投诉编号:</label>
                        <div class="col-sm-9">
                            <input type="text" class="form-control">
                        </div>
                    </div>

                    <div class="form-group form-group-center" style="padding-top:8px;">
                        <label for="inputPassword" class="col-sm-3 control-label padding-txt">订单编号:</label>
                        <div class="col-sm-9">
                            <input type="text" class="form-control">
                        </div>
                    </div>
                    <div class="form-group form-group-center" style="padding-top:8px;">
                        <label for="inputPassword" class="col-sm-3 control-label padding-txt">投诉人电话:</label>
                        <div class="col-sm-9">
                            <input type="text" class="form-control">
                        </div>
                    </div>
                    
                </div>
                <div class="col-md-6">
                    <div class="form-group">
                        <label for="inputPassword" class="col-sm-3 control-label padding-txt">投诉状态:</label>
                        <div class="col-sm-9">
                            <select class="form-control" placeholder="Username">
                                <option>处理中</option>
                                <option>处理完毕</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group form-group-center" style="padding-top:8px;">
                        <label for="inputPassword" class="col-sm-3 control-label padding-txt">投诉类型:</label>
                        <div class="col-sm-9">
                            <select class="form-control" placeholder="Username">
                                <option>物品缺失</option>
                                <option>物品送错</option>
                            </select>
                        </div>
                    </div>
                    <div class="form-group form-group-center" style="padding-top:8px;">
                        <label for="inputPassword" class="col-sm-3 control-label padding-txt">投诉日期:</label>
                        <div class="col-sm-4">
                            <input type="text" class="form-control">
                        </div>
                        <div class="col-sm-1 padding-txt">
                            到
                        </div>
                        <div class="col-sm-4">
                            <input type="text" class="form-control">
                        </div>
                    </div>
                </div>

                <div class="form-actions">
                    <div class="row">
                        <div class="col-md-offset-3 col-md-9">
                            <button type="submit" class="btn green">搜索订单</button>
                            <button type="button" class="btn default">批量导出</button>
                        </div>
                    </div>
                </div>
            </form>
        </div>
    </div>

    <!---投诉列表--->

    <table class="table table-striped">
        <thead>
            <tr>
                <th>投诉编号</th>
                <th>投诉状态</th>
                <th>投诉类型</th>
                <th>投诉人</th>
                <th>联系电话</th>
                <th>记录人</th>
                <th>投诉日期</th>
                <th>投诉内容</th>
                <th>最后处理</th>


                <th>操作</th>
            </tr>
        </thead>
