<div class="container-fluid">
    <div class="row footer-row1" style="padding-top:5px;">
        <form>
            <div class="col-md-6">
                <div class="form-group">
                    <label for="inputPassword" class="col-sm-3 control-label padding-txt">订单名称:</label>
                    <div class="col-sm-9">
                        <input type="text" class="form-control">
                    </div>
                </div>

                <div class="form-group form-group-center" style="padding-top:8px;">
                    <label for="inputPassword" class="col-sm-3 control-label padding-txt">订单编号:</label>
                    <div class="col-sm-9">
                        <input type="text" class="form-control">
                    </div>
                </div>
                <div class="form-group form-group-center" style="padding-top:8px;">
                    <label for="inputPassword" class="col-sm-3 control-label padding-txt">买家名称:</label>
                    <div class="col-sm-9">
                        <input type="text" class="form-control">
                    </div>
                </div>
                <div class="form-group form-group-center" style="padding-top:8px;">
                    <label for="inputPassword" class="col-sm-3 control-label padding-txt">退货:</label>
                    <div class="col-sm-9">
                        <select class="form-control" placeholder="Username">
                            <option>已退货</option>
                            <option>退货退款</option>
                        </select>
                    </div>
                </div>
            </div>
            <div class="col-md-6">
                <div class="form-group">
                    <label for="inputPassword" class="col-sm-3 control-label padding-txt">订单状态:</label>
                    <div class="col-sm-9">
                        <select class="form-control" placeholder="Username">
                            <option>待发货</option>
                            <option>已发货</option>
                        </select>
                    </div>
                </div>
                <div class="form-group form-group-center" style="padding-top:8px;">
                    <label for="inputPassword" class="col-sm-3 control-label padding-txt">投诉:</label>
                    <div class="col-sm-9">
                        <select class="form-control" placeholder="Username">
                            <option>发货慢</option>
                            <option>服务差</option>
                        </select>
                    </div>
                </div>
                <div class="form-group form-group-center" style="padding-top:8px;">
                    <label for="inputPassword" class="col-sm-3 control-label padding-txt">下单时间:</label>
                    <div class="col-sm-4">
                        <input type="text" class="form-control">
                    </div>
                    <div class="col-sm-1 padding-txt">
                        到
                    </div>
                    <div class="col-sm-4">
                        <input type="text" class="form-control">
                    </div>
                </div>
                <div class="form-group form-group-center" style="padding-top:8px;">
                    <label for="inputPassword" class="col-sm-3 control-label padding-txt">送货时间:</label>
                    <div class="col-sm-4">
                        <input type="text" class="form-control">
                    </div>
                    <div class="col-sm-1 padding-txt">
                        到
                    </div>
                    <div class="col-sm-4">
                        <input type="text" class="form-control">
                    </div>
                </div>
            </div>

            <div class="form-actions">
                <div class="row">
                    <div class="col-md-offset-3 col-md-9">
                        <button type="submit" class="btn green">搜索订单</button>
                        <button type="button" class="btn default">批量导出</button>
                    </div>
                </div>
            </div>
        </form>
    </div>
</div>

